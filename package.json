{
  "name": "aws-serverless-to-server",
  "version": "0.0.1",
  "description": "Runs your AWS HTTP Lambdas on lightweight and fast Express.js server environment with just one function.",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "ts-node example/server.ts",
    "tsc": "tsc",
    "tsc:v": "tsc -v",
    "tsc:watch": "tsc -w",
    "tsc:noEmit": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js \"./**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js \"./**/*.ts\" --fix",
    "lint:staged": "lint-staged",
    "test": "jest --forceExit --detectOpenHandles --expand --no-cache --coverage --config=./.jestrc.js",
    "test:watch": "npm test -- --watch",
    "test:clear-jest-cache": "rm -rf $(jest --showConfig | jq '.config[0].cacheDirectory')",
    "readme": "sh ./bin/create-readme.sh -a false -c false -l false -m true && cat README.md > README.tmp && node ./readme/readmePrefix && cat README.tmp >> README.md && rm -f README.tmp",
    "outdated:fix": "npx npm-check-updates -u --packageFile package.json",
    "doctoc": "node_modules/.bin/doctoc ./readme/additional-docs --github --title \"## Table of Contents\" --maxlevel 3",
    "docs": "npm run readme && npm run doctoc",
    "ts-prune": "npx ts-prune",
    "ts-prune-interfaces": "npx ts-prune | grep ' - I.*'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deadratfink/aws-serverless-as-server.git"
  },
  "keywords": [
    "aws",
    "api",
    "gateway",
    "http",
    "lambda",
    "openapi",
    "swagger"
  ],
  "author": {
    "name": "Jens Krefeldt",
    "email": "jens.krefeldt@veolia.com",
    "url": "https://github.com/deadratfink"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deadratfink/aws-serverless-as-server/issues"
  },
  "homepage": "https://github.com/deadratfink/aws-serverless-as-server",
  "dependencies": {
    "@apidevtools/openapi-schemas": "2.x",
    "@types/swagger-ui-express": "~4.1.3",
    "compression": "1.x",
    "cors": "2.x",
    "express": "4.x",
    "morgan": "1.x",
    "openapi3-ts": "4.x",
    "swagger-ui-express": "~5.0.0",
    "uuid": "9.x"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.x",
    "@types/aws-lambda": "8.x",
    "@types/cli": "0.x",
    "@types/compression": "1.x",
    "@types/cors": "2.x",
    "@types/express": "4.x",
    "@types/jest": "29.x",
    "@types/morgan": "1.x",
    "@types/node": "20.x",
    "@types/uuid": "9.x",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "aws-lambda": "1.x",
    "cli": "1.x",
    "colors": "1.x",
    "doctoc": "2.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-filenames": "1.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "27.x",
    "eslint-plugin-jest-async": "1.x",
    "eslint-plugin-jsdoc": "46.x",
    "eslint-plugin-prettier": "4.x",
    "husky": "8.x",
    "jest": "29.x",
    "lint-staged": "13.x",
    "npm-check-updates": "16.x",
    "prettier": "3.x",
    "ts-jest": "29.x",
    "ts-node": "10.x",
    "typescript": "5.x"
  }
}
